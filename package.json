{
  "name": "package-collector-doc",
  "version": "1.0.0",
  "description": "Summarize consumed packages as documentation",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .jsx,.js app/",
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "doc": "styleguidist server",
    "doc:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cange/package-collector-doc.git"
  },
  "author": "Christian Angermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cange/package-collector-doc/issues"
  },
  "homepage": "https://github.com/cange/package-collector-doc#readme",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.6",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "react-styleguidist": "^6.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/components/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^enzymeSetup$": "<rootDir>/app/__config__/enzymeSetup.js"
    }
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "IE 11"
  ]
}
